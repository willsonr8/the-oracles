{
  "routes": [
    {
      "route": "/api/*",
      "methods": [
        "get",
        "post"
      ],
      "rewrite": "https://the-oracles-v1.azurewebsites.net/api/:*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/httpKeyword",
      "methods": [
        "get",
        "post"
      ],
      "redirect": "https://the-oracles-v1.azurewebsites.net/api/httpKeyword",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ]
}